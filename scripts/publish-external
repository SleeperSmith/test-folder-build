#!/bin/bash

docker login -u $BC_DH_USERNAME -p $BC_DH_PASSWORD
BC_TAG_NAME=${BC_DOCKER_IMAGE_NAME}:${GO_PIPELINE_LABEL}.${GO_STAGE_COUNTER}
docker push ${BC_PVTDEV_REGISTRY}/${BC_TAG_NAME}

docker tag ${BC_PVTDEV_REGISTRY}/${BC_TAG_NAME} ${BC_PVTDEV_REGISTRY}/${BC_DOCKER_IMAGE_NAME}
docker push ${BC_PVTDEV_REGISTRY}/${BC_DOCKER_IMAGE_NAME}